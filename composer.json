{
    "name": "inggo/psba-manila",
    "authors": [
        {
            "name": "Inggo Espinosa",
            "email": "inggo.espinosa@gmail.com"
        }
    ],
    "keywords": [
        "theme",
        "psba-manila"
    ],
    "type": "wordpress-theme",
    "license": "GPL-2.0-or-later",
    "require": {
        "wpupdatephp/wp-update-php": "^1.1",
        "cmb2/cmb2": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Inggo\\WordPress\\": "wp/"
        },
        "files": [
            "vendor/cmb2/cmb2/init.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}/": ["cmb2/cmb2"]
        }
    }        
}
